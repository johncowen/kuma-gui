{
  "name": "kuma-gui",
  "version": "2.6.0",
  "private": true,
  "description": "Kuma Manager",
  "author": "Kong",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite -c ./vite.config.development.ts",
    "preview": "npm run build:staging && npm run preview:built",
    "preview:built": "vite preview -c ./vite.config.preview.ts",
    "build": "vite build -c ./vite.config.production.ts",
    "build:ci": "npm run build -- --mode preview -c ./vite.config.development.ts",
    "build:gui": "mkdir gui && mv dist/* gui && mv gui dist/gui && mv dist/gui/mockServiceWorker.js dist/",
    "build:preview": "npm run build:ci && npm run build:gui && cp _redirects dist/_redirects",
    "build:staging": "cross-env VITE_ZONE_CREATION_FLOW=enabled npm run build && npm run  build:gui",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:lockfile": "lockfile-lint --path package-lock.json --allowed-hosts npm --validate-https",
    "lint:ts": "vue-tsc --noEmit",
    "lint:styles": "stylelint --allow-empty-input './src/**/*.{css,scss,vue}'",
    "_vitest": "cross-env TZ=UTC FORCE_COLOR=1 vitest -c vite.config.production.ts",
    "_cypress": "cross-env TZ=UTC cypress",
    "test": "npm run test:unit",
    "test:unit": "npm run _vitest run",
    "test:unit:coverage": "npm run _vitest -- --coverage",
    "test:watch": "npm run _vitest -- --watch",
    "test:browser": "npm run _cypress -- run --browser chrome",
    "test:browser:view": "cross-env KUMA_BASE_URL=http://localhost:8080/gui npm run _cypress -- open --e2e --browser chrome",
    "docs:generate": "rm -rf docs/components && vue-docgen src/**/*.vue",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.9",
    "npm": "^10.0.0"
  },
  "dependencies": {
    "@kong-ui-public/i18n": "^2.1.6",
    "@kong-ui-public/app-layout": "^4.2.3",
    "@kong/icons": "^1.13.0",
    "@kong/kongponents": "^9.0.0-alpha < 9.0.0-beta",
    "@vueuse/core": "^10.10.0",
    "brandi": "^5.0.0",
    "deepmerge": "^4.3.1",
    "js-yaml": "^4.1.0",
    "object.groupby": "^1.0.3",
    "path-to-regexp": "^6.2.2",
    "pretty-bytes": "^6.1.1",
    "prismjs": "^1.29.0",
    "semver": "^7.6.2",
    "vue": "^3.4.26",
    "vue-github-button": "^3.1.0",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.6",
    "@badeball/cypress-cucumber-preprocessor": "^20.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.1",
    "@faker-js/faker": "^8.4.1",
    "@kong/design-tokens": "^1.12.12",
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/prismjs": "^1.26.4",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@whyframe/core": "^0.1.11",
    "@whyframe/vue": "^0.1.7",
    "autoprefixer": "^10.4.19",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "cross-env": "^7.0.3",
    "cypress": "^13.9.0",
    "cypress-fail-fast": "^7.1.0",
    "cypress-fail-on-console-error": "^5.1.1",
    "dotenv": "^16.4.5",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.26.0",
    "glob": "^10.4.1",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "lockfile-lint": "^4.13.2",
    "marked": "^12.0.2",
    "msw": "^2.3.1",
    "postcss": "^8.4.38",
    "postcss-html": "^1.7.0",
    "sass": "^1.77.4",
    "shiki": "^1.5.1",
    "standard": "^17.1.0",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "^5.4.5",
    "urlpattern-polyfill": "^10.0.0",
    "vite": "^5.2.13",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "^1.2.2",
    "vitest": "^1.6.0",
    "vue-docgen-cli": "^4.79.0",
    "vue-tsc": "^2.0.21"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "msw": {
    "workerDirectory": "public"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "./cypress/main.ts",
      "./cypress/support/step_definitions/*.ts"
    ]
  }
}
