{
  "name": "kuma-gui",
  "version": "2.6.0",
  "private": true,
  "description": "Kuma Manager",
  "author": "Kong",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "deps": "cypress install",
    "dev": "yarn run dev:mock-api",
    "dev:mock-api": "vite -c ./vite.config.development.ts",
    "dev:real-api": "cross-env VITE_MOCK_API_ENABLED=false yarn dev",
    "preview": "yarn build:staging && yarn preview:built",
    "preview:built": "vite preview -c ./vite.config.preview.ts",
    "build": "vite build -c ./vite.config.production.ts",
    "build:ci": "yarn run build --mode preview -c ./vite.config.development.ts",
    "build:gui": "mkdir gui && mv dist/* gui && mv gui dist/gui && mv dist/gui/mockServiceWorker.js dist/",
    "build:preview": "yarn run build:ci && yarn build:gui && cp _redirects dist/_redirects",
    "build:staging": "cross-env VITE_ZONE_CREATION_FLOW=enabled yarn build && yarn build:gui",
    "lint": "eslint --ext .js,.ts,.vue --fix .",
    "lint:lockfile": "lockfile-lint --path yarn.lock --allowed-hosts yarn --validate-https",
    "lint:ts": "vue-tsc --noEmit",
    "lint:styles": "stylelint --allow-empty-input './src/**/*.{css,scss,vue}'",
    "test": "cross-env TZ=UTC yarn run test:unit",
    "test:browser": "cross-env TZ=UTC cypress run --browser chrome",
    "test:browser:view": "cross-env TZ=UTC KUMA_BASE_URL=http://localhost:8080/gui cypress open --e2e --browser chrome",
    "test:unit": "cross-env FORCE_COLOR=1 vitest -c vite.config.production.ts run",
    "test:unit:coverage": "cross-env FORCE_COLOR=1 vitest -c vite.config.production.ts run --coverage",
    "test:watch": "vitest -c vite.config.production.ts --watch",
    "docs:dev": "vitepress dev",
    "docs:generate": "rm -rf docs/components && vue-docgen src/**/*.vue",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "engines": {
    "node": ">=20.9",
    "yarn": "^1.22.0"
  },
  "dependencies": {
    "@kong-ui-public/i18n": "2.1.2",
    "@kong/icons": "1.8.14",
    "@kong/kongponents": "9.0.0-pr.2020.19de55b2.0",
    "@vueuse/core": "10.7.2",
    "brandi": "5.0.0",
    "deepmerge": "4.3.1",
    "js-yaml": "4.1.0",
    "pretty-bytes": "6.1.1",
    "prismjs": "1.29.0",
    "semver": "7.6.0",
    "vue": "3.4.18",
    "vue-github-button": "3.1.0",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-typescript": "7.23.3",
    "@badeball/cypress-cucumber-preprocessor": "20.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "2.2.0",
    "@faker-js/faker": "8.4.1",
    "@kong/design-tokens": "1.12.7",
    "@modyfi/vite-plugin-yaml": "1.1.0",
    "@types/glob": "8.1.0",
    "@types/js-yaml": "4.0.9",
    "@types/marked": "6.0.0",
    "@types/prismjs": "1.26.3",
    "@types/semver": "7.5.7",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/coverage-istanbul": "1.2.2",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "2.4.4",
    "@whyframe/core": "0.1.10",
    "@whyframe/vue": "0.1.6",
    "autoprefixer": "10.4.17",
    "babel-plugin-transform-import-meta": "2.2.1",
    "babel-plugin-transform-vite-meta-env": "1.0.3",
    "cross-env": "7.0.3",
    "cypress": "13.6.4",
    "cypress-fail-fast": "7.1.0",
    "dotenv": "16.4.3",
    "esbuild": "0.20.0",
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-vue": "9.21.1",
    "glob": "10.3.10",
    "jsdom": "24.0.0",
    "lockfile-lint": "4.13.1",
    "marked": "12.0.0",
    "msw": "2.1.5",
    "postcss-html": "1.6.0",
    "sass": "1.70.0",
    "standard": "17.1.0",
    "stylelint": "16.2.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended-scss": "14.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "typescript": "5.3.3",
    "urlpattern-polyfill": "10.0.0",
    "vite": "5.1.1",
    "vite-svg-loader": "5.1.0",
    "vitepress": "1.0.0-rc.42",
    "vitest": "1.2.2",
    "vue-docgen-cli": "4.67.0",
    "vue-tsc": "1.8.27"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "msw": {
    "workerDirectory": "public"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "./cypress/main.ts",
      "./cypress/support/step_definitions/*.ts"
    ]
  }
}
